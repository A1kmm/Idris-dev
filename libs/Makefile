build: .PHONY
	make -C prelude build
	make -C base build
	make -C effects build
	make -C javascript build


install: .PHONY
	make -C prelude install
	make -C base install
	make -C effects install
	make -C javascript install

clean: .PHONY
	make -C prelude clean
	make -C base clean
	make -C effects clean
	make -C javascript clean
        
.PHONY:
